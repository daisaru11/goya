{
    "name": "dsaru/goya",
    "description": "Database Schema Diff Tool for Laravel",
    "authors": [
        {
            "name": "daisaru11",
            "email": "daisaru11@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "doctrine/dbal": "2.3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "dev-master"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Dsaru\\Goya\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
